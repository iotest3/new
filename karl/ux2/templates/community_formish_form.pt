<metal:page use-macro="main_template">
  <div metal:fill-slot="content">

    ${panel('status_message')}
    ${panel('error_message')}
    ${panel('actions')}
    <h1>${layout.page_title}</h1>
    <p tal:content="page_description|''"/>
    <span tal:replace="structure request.form()" />
    
  </div>
  
</metal:page>
