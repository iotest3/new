<div class="sras-result">
  <a class="sras-result-title" href="${r['url']}">${r['title']} Office</a>
  <p class="sras-summary">
    <div>${doc.telephone}</div>
    <div>${doc.address}</div>
    <div>
      ${doc.city}
      <span tal:condition="doc.state" tal:replace="string:, ${doc.state}" />
    </div>
    <div>${doc.zipcode}</div>
  </p>
</div>


